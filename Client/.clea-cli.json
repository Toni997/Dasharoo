{
  "type": "application",
  "name": "dasharoo",
  "root": "src",
  "main": "app/app.module.ts",
  "styles": [
    "styles/main.scss",
    "styles/abstracts/_variables.scss"
  ],
  "environmentSource": "config/config.json",
  "environments": {
    "development": "config/config.dev.json",
    "production": "config/config.prod.json"
  },
  "karmaConfig": "./karma.conf.js",
  "performance": {
    "hints": "warning",
    "maxEntrypointSize": 2000000,
    "maxAssetSize": 2000000
  },
  "spec": {
    "component": false,
    "directive": false,
    "filter": false,
    "service": false,
    "module": true
  }
}
